                H                           HLPPNLGRP('USER' IMAPVW)
                                            HLPARA(*FLD W0USER)
                H                           HLPPNLGRP('PASS' IMAPVW)
                                            HLPARA(*FLD W0PASS)
                H                           HLPPNLGRP('TLS' IMAPVW)
                                            HLPARA(*FLD W0UTLS)
                H                           HLPPNLGRP('PORT' IMAPVW)
                                            HLPARA(*FLD W0PORT)
                H                           HLPPNLGRP('IMAPVWW0' IMAPVW)
                                            HLPARA(*RCD)

                  W0CROW         3S 0H
                  W0CCOL         3S 0H
                  W0CRCD        10A  H
                  W0CFLD        10A  H
                  W0CPOS         4S 0H

                  W0CHEAD       30A  O  1  2MSGID(C000009 *LIBL/IMAPMSG)
