                  W0CUSER        9A  O  4  2MSGID(C000011 *LIBL/IMAPMSG)
                  W0CPASS        9A  O  5  2MSGID(C000012 *LIBL/IMAPMSG)
                  W0CUTLS        9A  O  6  2MSGID(C000013 *LIBL/IMAPMSG)
                  W0CPORT        9A  O  7  2MSGID(C000016 *LIBL/IMAPMSG)
                  W0CFOOT       30A  O  9  2MSGID(C000014 *LIBL/IMAPMSG)
                                            COLOR(BLU)
                  W0HOST        32A  B  3 12COLOR(TRQ) ALIAS(W0_HOST)
